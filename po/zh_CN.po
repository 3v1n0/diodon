#: ../data/preferences.ui.h:10
msgid "_Automatically paste selected item"
msgstr "自动粘贴所选条目(_A)"

#: ../data/preferences.ui.h:14
msgid "Clipboard"
msgstr "剪贴板"

#: ../data/preferences.ui.h:17
msgid "Hotkeys"
